# ============================================
# СКОПИРУЙТЕ ЭТОТ ФАЙЛ В .env
# Команда: cp .env.example .env
# ============================================

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_WEBHOOK_URL=
TELEGRAM_ALLOWED_IDS=

# Supabase Configuration
SUPABASE_URL=https://your_project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_key_here

# Application Configuration
NODE_ENV=development
PORT=3000
WEBAPP_URL=

# Assistant tuning (опционально)
ASSISTANT_SUCCESS_THRESHOLD=75
ASSISTANT_SLUMP_THRESHOLD=45

# Security (оставьте как есть или измените на свои случайные строки)
ENCRYPTION_SECRET=training_bot_encryption_secret_key_32ch
JWT_SECRET=training_bot_jwt_secret_key_for_auth_32
